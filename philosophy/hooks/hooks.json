{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo 'moo.md active. Clarify intent → /plan → proceed. Learnings: ~/.claude/learnings/'"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash ${CLAUDE_PLUGIN_ROOT}/scripts/pre_compact.sh"
          }
        ]
      }
    ]
  }
}
